{
    "name": "mrpunyapal/filament-infinite-select",
    "description": "A Filament Select component with infinite scroll lazy loading for options",
    "keywords": [
        "MrPunyapal",
        "laravel",
        "filament",
        "select",
        "infinite-scroll",
        "lazy-loading"
    ],
    "homepage": "https://github.com/mrpunyapal/filament-infinite-select",
    "support": {
        "issues": "https://github.com/mrpunyapal/filament-infinite-select/issues",
        "source": "https://github.com/mrpunyapal/filament-infinite-select"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Punyapal Shah",
            "email": "mrpunyapal@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MrPunyapal\\FilamentInfiniteSelect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MrPunyapal\\FilamentInfiniteSelect\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrPunyapal\\FilamentInfiniteSelect\\FilamentInfiniteSelectServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}